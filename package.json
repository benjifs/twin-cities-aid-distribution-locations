  "devDependencies": {
    "@babel/preset-env": "^7.10.2",
    "@snowpack/plugin-babel": "^1.0.0",
    "@snowpack/plugin-dotenv": "^1.1.1",
    "@snowpack/plugin-webpack": "^1.1.0",
    "got": "^11.3.0",
    "netlify-lambda": "^1.6.3",
